{
  "name": "mzfmt",
  "displayName": "mzfmt",
  "description": "Pretty printing for SQL (Materialize edition).",
  "version": "0.0.4",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "repository": "github:maddyblue/mzfmt",
  "publisher": "maddyblue",
  "browser": "./dist/web/extension.js",
  "activationEvents": [
    "onCommand:mzfmt.formatSQL",
    "onLanguage:sql"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sql",
        "extensions": [
          "sql"
        ]
      }
    ],
    "commands": [
      {
        "command": "mzfmt.formatSQL",
        "title": "Format SQL"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/assert": "^1.5.6",
    "@types/mocha": "^9.1.1",
    "@types/vscode": "^1.74.0",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-web": "^0.0.29",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "assert": "^2.0.0",
    "eslint": "^8.20.0",
    "mocha": "^10.0.0",
    "process": "^0.11.10",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.2",
    "vscode-web-wasm-webpack-plugin": "file:../vscode-web-wasm-webpack-plugin",
    "webpack": "file:../webpack",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "mzfmt-wasm": "file:../../web/pkg"
  }
}
